        "jsonwebtoken": "^9.0.0",
        "ldapts": "^7.0.12",
        "lottie-web-vue": "^2.0.7",
        "marked": "^12.0.0",
        "mqtt": "^5.1.1",
        "mqtt-match": "^3.0.0",
        "nodemailer": "^6.9.3",
        "pg": "^8.11.3",
        "pino": "^8.15.1",
        "pino-pretty": "^10.0.0",
        "posthog-node": "^4.2.0",
        "qrcode": "^1.5.3",
        "random-words": "~2.0.0",
        "semver": "~7.6.0",
        "sequelize": "^6.33.0",
        "sqlite3": "^5.1.6",
        "stripe": "^8.222.0",
        "uuid": "^9.0.0",
        "vue": "^3.4.5",
        "vue-router": "^4.2.5",
        "vue-shepherd": "^3.0.0",
        "vuex": "^4.1.0",
        "yaml": "^2.3.1",
        "zxcvbn": "^4.4.2"
    },
    "devDependencies": {
        "@babel/core": "^7.22.15",
        "@babel/preset-env": "^7.22.15",
        "@vitejs/plugin-vue": "^5.0.2",
        "@vitest/coverage-istanbul": "^1.1.0",
