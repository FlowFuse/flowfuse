        "fastify-secrets-core": "^1.1.0",
        "fastify-secrets-env": "^1.0.1",
        "fastify-sensible": "^3.1.0",
        "fastify-static": "^4.2.2",
        "handlebars": "^4.7.7",
        "hashids": "^2.2.8",
        "jsonwebtoken": "^8.5.1",
        "lru-cache": "^6.0.0",
        "nodemailer": "^6.7.0",
        "pino-pretty": "^5.1.2",
        "sequelize": "^6.6.4",
