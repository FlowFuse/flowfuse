{
    "name": "@flowforge/flowforge",
    "version": "0.2.0",
    "description": "An open source low-code development platform",
    "scripts": {
        "start": "node forge/app.js",
        "build": "webpack -c ./config/webpack.config.js",
        "serve": "npm-run-all --parallel build-watch start-watch",
        "start-watch": "NODE_ENV=development nodemon -w forge -w ee/forge -i forge/containers/localfs_root forge/app.js",
        "build-watch": "webpack --mode=development -c ./config/webpack.config.js --watch",
        "docs": "jsdoc -c ./config/jsdoc.json",
        "lint": "eslint -c .eslintrc 'forge/**/*.js' 'frontend/**/*.js' 'test/**/*.js' --ignore-pattern 'frontend/dist/**'",
        "lint:fix": "eslint -c .eslintrc 'forge/**/*.js' 'frontend/**/*.js' 'test/**/*.js' --ignore-pattern 'frontend/dist/**' --fix",
        "test": "npm run lint && npm run test:unit && npm run test:system",
        "test:unit": "mocha test/unit/**/*_spec.js",
        "test:system": "mocha test/system/**/*_spec.js"
    },
    "bin": {
        "flowforge": "./forge/app.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/flowforge/flowforge.git"
    },
    "author": {
        "name": "FlowFlow Inc."
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/flowforge/flowforge/issues"
    },
    "homepage": "https://github.com/flowforge/flowforge#readme",
    "dependencies": {
        "@aws-sdk/client-ses": "^3.49.0",
        "@aws-sdk/credential-provider-node": "^3.49.0",
        "@flowforge/localfs": "^0.2.0",
        "@headlessui/vue": "1.4.3",
        "@heroicons/vue": "1.0.5",
        "axios": "^0.25.0",
        "bcrypt": "^5.0.1",
        "cronosjs": "^1.7.1",
        "fastify": "^3.27.0",
        "fastify-autoload": "^3.10.0",
        "fastify-cookie": "^5.5.0",
        "fastify-csrf": "^3.1.0",
        "fastify-plugin": "^3.0.1",
        "fastify-static": "^4.5.0",
        "handlebars": "^4.7.7",
        "hashids": "^2.2.10",
        "jsonwebtoken": "^8.5.1",
        "lru-cache": "^6.0.0",
        "marked": "^4.0.12",
        "nodemailer": "^6.7.2",
        "pino-pretty": "^5.1.3",
        "semver": "^7.3.5",
        "sequelize": "^6.15.0",
        "sqlite3": "^5.0.2",
        "stripe": "^8.205.0",
        "uuid": "^8.3.2",
        "vue": "^3.2.27",
        "vue-router": "^4.0.12",
        "vuex": "^4.0.2",
        "yaml": "^1.10.2"
    },
    "devDependencies": {
        "@babel/core": "^7.16.10",
        "@babel/preset-env": "^7.16.10",
        "@tailwindcss/forms": "^0.3.4",
        "autoprefixer": "^10.4.2",
        "babel-loader": "^8.2.3",
        "clean-jsdoc-theme": "^3.3.1",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^9.1.0",
        "css-loader": "^5.2.7",
        "dotenv-webpack": "^7.1.0",
        "eslint": "^7.11.0",
        "eslint-config-standard": "^16.0.1",
        "eslint-import-resolver-node": "^0.3.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-vue": "^8.4.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "jsdoc": "^3.6.7",
        "mini-css-extract-plugin": "^2.5.3",
        "mocha": "^9.1.4",
        "mocha-cli": "^1.0.1",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.6",
        "postcss-loader": "^6.2.1",
        "postcss-preset-env": "^6.7.0",
        "should": "^13.2.3",
        "style-loader": "^3.3.1",
        "tailwindcss": "^2.2.19",
        "vue-loader": "^16.8.3",
        "vue-template-compiler": "^2.6.14",
        "webpack": "^5.68.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^3.11.3"
    },
    "engines": {
        "node": ">=16.x"
    }
}
